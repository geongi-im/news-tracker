# Google Gemini API
GOOGLE_API_KEY=your_gemini_api_key_here

# BASE URL
BASE_URL=http://example.com

# ===== Gemini API 설정 =====
GEMINI_MAX_RETRIES=5
GEMINI_RETRY_DELAY=5
GEMINI_MAX_DELAY=60
GEMINI_SUCCESS_DELAY=3

# ===== REST API 설정 =====
REST_MAX_RETRIES=3
REST_RETRY_DELAY=1.0

# ===== 요청 지연 설정 (초) =====
REQUEST_DELAY=0.2    # 중복 체크 사이 지연
BATCH_DELAY=1.0      # 피드 간 지연
